<block>
  <name>FFT Estimator CC</name>
  <key>dslwp_fft_estimator_cc</key>
  <category>[DSLWP]</category>
  <import>import dslwp</import>
  <make>dslwp.fft_estimator_cc($fft_size, $threshold, $tap_len)</make>
  <param>
    <name>FFT Size</name>
    <key>fft_size</key>
    <type>raw</type>
  </param>
  <param>
    <name>Threshold</name>
    <key>threshold</key>
    <type>float</type>
  </param>
  <param>
    <name>Tap Length</name>
    <key>tap_len</key>
    <type>raw</type>
  </param>
  <sink>
    <name>in</name>
    <type>complex</type>
    <vlen>$fft_size</vlen>
    <nports>2</nports>
  </sink>
  <source>
    <name>out</name>
    <type>complex</type>
  </source>
</block>
